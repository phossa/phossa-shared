{
    "name": "phossa/phossa-shared",
    "version": "1.0.5",
    "license": "MIT",
    "type": "library",
    "description": "The shared package of Phossa framework",
    "keywords": ["phossa", "framework"],
    "homepage": "https://github.com/phossa/phossa-shared",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Shared\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Phossa\\Shared\\": "tests/src/" }
    },
    "require": {
        "php": ">=5.4.0"
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml",  "composer.lock"]
    }
}